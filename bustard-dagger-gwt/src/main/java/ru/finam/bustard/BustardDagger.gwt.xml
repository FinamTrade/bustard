<module rename-to="bastardDagger">
    <inherits name="com.google.gwt.core.Core"/>
    <inherits name="ru.finam.bustard.Bustard"/>
    <inherits name="dagger.Dagger"/>
    <inherits name="com.google.gwt.user.DOM"/>

    <source path="">
        <exclude name="**/codegen/**"/>
        <exclude name="**/java/**"/>
    </source>

    <set-property name="user.agent" value="safari"/>

    <generate-with class="ru.finam.bustard.codegen.ChannelModuleGwtGenerator">
        <when-type-assignable class="ru.finam.bustard.ChannelModule" />
    </generate-with>
</module>
<module rename-to="bustard">
    <inherits name="com.google.gwt.core.Core" />
    <inherits name="com.google.common.collect.Collect"/>
    <inherits name="com.google.gwt.user.ClippedImage"/>

    <source path="">
        <exclude name="**/codegen/**" />
        <exclude name="**/java/**" />
    </source>

    <generate-with class="ru.finam.bustard.codegen.BustardGwtGenerator">
        <when-type-assignable class="ru.finam.bustard.Bustard" />
    </generate-with>
</module>